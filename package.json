{
  "name": "@ordis-dev/ordis",
  "version": "0.1.0",
  "type": "module",
  "description": "Schema-first LLM extraction tool that turns unstructured text into validated structured data. Works with Node.js, Deno, and Bun.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ordis": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "benchmark": "tsx benchmarks/runner.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ordis-dev/ordis.git"
  },
  "keywords": [
    "llm",
    "extraction",
    "schema",
    "validation",
    "cli",
    "openai",
    "ollama",
    "cross-platform",
    "deno",
    "bun",
    "nodejs",
    "webstandards",
    "typescript",
    "javascript",
    "jsonschema"
  ],
  "author": "Ordis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ordis-dev/ordis/issues"
  },
  "homepage": "https://github.com/ordis-dev/ordis#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@vitest/ui": "^4.0.15",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
